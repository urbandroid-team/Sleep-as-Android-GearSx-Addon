<resources>
    <application name="SleepAsAndroidProvider" >
        <serviceProfile
            id="/system/sleepassamsung"
            name="sleepassamsung"
            role="provider"
            serviceImpl="com.urbandroid.sleep.addon.generic.samsung.SleepAsAndroidProviderService"
            version="1.0"
            serviceLimit="ANY"
	        serviceTimeout="10">

            <supportedTransports>
                <transport type="TRANSPORT_BT" />
            </supportedTransports>

            <serviceChannel
                id="1750"
                dataRate="low"
                priority="low"
                reliability= "enable"/>
        </serviceProfile>
    </application>
</resources>